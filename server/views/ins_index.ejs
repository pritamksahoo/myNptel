	<div class="modal fade" id="modal_create_course" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header" style="color: #123456">
        Add a New Course
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
      	<form method="post" action="/addnewcourse">
		  <div class="form-group">
			<label for="c1">Course Name</label>
			<input id="c1" name="course_name" type="text" class="form-control" placeholder="Enter course name">
			<small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
		  </div>
		  <div class="form-group">
			<label for="t1">Topic</label>
				<div class="row-fluid">
				  <select id="t1" name="topic_name" class="selectpicker" data-show-subtext="true" data-live-search="true">
				  	 <% for (var i=0; i<course_topics.length; i++) { %>
					<option value="<%= course_topics[i].topic_name %>"><%= course_topics[i].topic_name %></option>
					 <% } %>
				  </select>
				</div>
		  </div>
		  <button type="button" class="btn btn-danger" data-dismiss="modal">Cancel</button>
		  <button type="submit" class="btn btn-success">Save & Next</button>
		</form>
      </div>
    </div>
  </div>
